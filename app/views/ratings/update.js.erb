$('#star').raty({
  readOnly: true,
  score: <%= @venue.average_rating %>,
  path: '/assets/mini'
});

$('#user_star').raty({
  score: <%= @rating.score %>,
  path: '/assets',
  click: function(score, evt) {
    $.ajax({
      url: '/ratings/' + <%= @rating.id %>,
      type: 'PATCH',
      data: { score: score, width:200px }
    }).done(function (data){});
  }
});
